{
  "name": "cosmeticure",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "build": "yarn prisma:generate && next build",
    "db:pull:postgres": "dotenv -e .env.development -- npx prisma db pull",
    "dev": "next dev",
    "lint": "next lint",
    "migrate:postgres": "dotenv -e .env.development -- npx prisma migrate dev --name init",
    "prepare": "husky",
    "prisma:generate": "prisma generate",
    "seed": "dotenv -e .env.development -- npx prisma db seed",
    "start": "next start",
    "test": "jest",
    "test:watch": "jest --watch",
    "upgrade": "ncu -u"
  },
  "dependencies": {
    "@appcorp/app-corp-designs": "^0.2.0",
    "@auth/prisma-adapter": "^2.1.0",
    "@heroicons/react": "^2.1.3",
    "@hookform/resolvers": "^3.4.2",
    "@material-tailwind/react": "^2.1.9",
    "@next/third-parties": "^14.2.3",
    "@prisma/client": "^5.14.0",
    "@react-pakistan/react-icon-collection": "^3.5.0",
    "@react-pakistan/util-functions": "^1.22.16",
    "@supabase/ssr": "^0.3.0",
    "@supabase/supabase-js": "^2.43.4",
    "next": "14.2.3",
    "next-auth": "^4.24.7",
    "next-intl": "^3.14.1",
    "react": "^18",
    "react-dom": "^18",
    "react-hook-form": "^7.51.5",
    "swr": "^2.2.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
